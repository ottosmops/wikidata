{
  "name": "survos/wikidata",
  "description": "A PHP client for working with Wikidata API.",
  "keywords": [
    "wikidata",
    "client",
    "php"
  ],
  "homepage": "https://github.com/survos/wikidata",
  "license": "MIT",
  "version": "4.0.0",
  "authors": [
    {
      "name": "Arhey",
      "role": "developer"
    },
    {
      "name": "tacman",
      "role": "developer"
    }
  ],
  "require": {
    "php":"^8.2",
    "laravel/framework": ">=9",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/http-client": "^6.4||^7.0"
  },
  "autoload": {
    "psr-4": {
      "Wikidata\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Wikidata\\Tests\\": "tests"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.10"
  }
}
